<header class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            {{ header }}
        </div>
    </div>
</header>

<section class="container communication paddingtop">
    <div class="row">
        <div class="col-sm-12">
            {% if highlighted %}
                {{ highlighted }}
            {% endif %}
        </div>
    </div>
</section>

<section class="container content-wrap paddingtop">
    <div class="row">
        <div class="col-sm-12">

            {{ page.content }}

        </div>
    </div>
</section>

<section class="container leftright paddingtop">
    <div class="row">
        <div class="col-sm-9">
            {% if sidebar_first %}
                {{ sidebar_first }}
            {% endif %}
        </div>
        <div class="col-sm-3">
            {% if sidebar_second %}
                {{ sidebar_second }}
            {% endif %}
        </div>
    </div>
</section>